      height: 120px;
      border-radius: 50%;
      object-fit: cover;
    }

    .card h3 {
      margin-top: 10px;
      font-size: 20px;
    }

    .btn {
      display: inline-block;
      margin-top: 15px;
      padding: 10px 15px;
      background-color: #1877f2;
      color: white;
      text-decoration: none;
      border-radius: 6px;
      font-weight: bold;
      font-family: Arial, Helvetica, sans-serif;
    }

    .btn img {
      width: 18px;
      height: 18px;
      vertical-align: middle;
      margin-right: 5px;
