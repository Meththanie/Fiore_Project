  <!-- AOS Library JS -->
  <script src="https://cdn.jsdelivr.net/npm/aos@2.3.4/dist/aos.js"></script>
  <!-- Page Script -->
  <script src="/app.js"></script>
  <script>
    AOS.init({ duration: 2500, delay: 300, once: true });
  </script>
  <script>
    document.addEventListener('DOMContentLoaded', () => {
  // For each gallery on the pageâ€¦
  document.querySelectorAll('.wed-gallery-container').forEach(container => {
    const gallery   = container.querySelector('.wed-gallery-items');
    const prevBtn   = container.querySelector('.prevBtn');
    const nextBtn   = container.querySelector('.nextBtn');
    const gap       = 16;

    // Compute slide distance dynamically
    function slideAmt() {
      const item = gallery.querySelector('.wed-gallery-item');
      return item.getBoundingClientRect().width + gap;
    }

    // Attach navigation handlers
    prevBtn.addEventListener('click', () => {
      gallery.scrollBy({ left: -slideAmt(), behavior: 'smooth' });
    });
    nextBtn.addEventListener('click', () => {
      gallery.scrollBy({ left:  slideAmt(), behavior: 'smooth' });
    });

    // Prevent manual scrolling
    gallery.addEventListener('wheel',     e => e.preventDefault());
    gallery.addEventListener('touchmove', e => e.preventDefault());
  });
});
  </script>
  
